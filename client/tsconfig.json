{
  "compileOnSave": false,
  "paths": {
    "@app/*": ["app/*"],
    "@env/*": ["environments/*"],
    "@modules/*": ["app/modules/*"],
    "@shared/*": ["app/shared/*"]
  },
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
